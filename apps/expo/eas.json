{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote",
    "requireCommit": false
  },
  "build": {
    "base": {
      "yarn": "4.12.0",
      "env": {
        "APP_ENV": "production"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "APP_ENV": "development",
        "BASE_URL": "https://nyangtodac-dev.site/",
        "IOS_WEBVIEW_URL": "https://nyangtodac-web-fe.pages.dev/",
        "ANDROID_WEBVIEW_URL": "https://nyangtodac-web-fe.pages.dev/"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "APP_ENV": "production"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
